@model VegeFoods_MVC.Models.OurService
<head>
    <link rel="stylesheet" href="~/css/flaticon.css">
</head>
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete - Our Service</h1>
<br />
<div>
    <h3>Are you sure you want to delete this?</h3>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SVGImage)
        </dt>
        <dd class = "col-sm-10">
            <span class="@Html.DisplayFor(model => model.SVGImage)"></span>
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Title)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Subtitle)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Subtitle)
        </dd>
    </dl>
</div>
@Html.Partial("_DeleteButtonSection")