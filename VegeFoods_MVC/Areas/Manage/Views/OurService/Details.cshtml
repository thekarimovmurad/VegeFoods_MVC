@model VegeFoods_MVC.Models.OurService
<head>
    <link rel="stylesheet" href="~/css/flaticon.css">
</head>
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SVGImage)
        </dt>
        <dd class = "col-sm-10">
            <span class="@Html.DisplayFor(model => model.SVGImage)"></span>
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Title)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Subtitle)
