</div>
<div>
    <a class="btn btn-warning btn-icon btn-sm mb-1" asp-action="Edit" asp-route-id="@Model?.Id"><i class="bi bi-pencil"></i></a>
    <a class="btn btn-danger btn-icon btn-sm mb-1" asp-action="Delete" asp-route-id="@Model?.Id"><i class="bi bi-trash"></i></a>
