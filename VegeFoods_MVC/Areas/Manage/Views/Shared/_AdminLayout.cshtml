<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8" />

	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<title>Admin Dashboard</title>
	<link rel="shortcut icon" href="~/admin/assets/images/favicon.svg" />
	<link rel="stylesheet" href="~/admin/assets/fonts/bootstrap/bootstrap-icons.css" />
	<link rel="stylesheet" href="~/admin/assets/css/main.min.css" />
	<link rel="stylesheet" href="~/admin/assets/vendor/overlay-scroll/OverlayScrollbars.min.css" />
</head>

<body>
	<div class="page-wrapper">
		<div class="main-container">
			<nav id="sidebar" class="sidebar-wrapper">
 				<div class="app-brand p-4">
					<a href="index.html">
						<img src="~/admin/assets/images/logo.svg" class="logo" alt="Bootstrap Gallery" />
					</a>
				</div>
				<div class="sidebarMenuScroll">
					<ul class="sidebar-menu">
						<li>
							<a asp-controller="AboutUs" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">About Us</span>
							</a>
						</li>
						<li>
							<a asp-controller="blog" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Blog</span>
							</a>
						</li>
						<li>
							<a asp-controller="blogtag" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Blog Tag</span>
							</a>
						</li>
						<li>
							<a asp-controller="tag" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Tag</span>
							</a>
						</li>
						<li>
							<a asp-controller="category" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Category</span>
							</a>
						</li>
						<li>
							<a asp-controller="Product" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Product</span>
							</a>
						</li>
						<li>
							<a asp-controller="size" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Size</span>
							</a>
						</li>
						<li>
							<a asp-controller="priceList" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Price</span>
							</a>
						</li>
						<li>
							<a asp-controller="Testomonial" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Testomonial</span>
							</a>
						</li>
						<li>
							<a asp-controller="slider" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Slider</span>
							</a>
						</li>
						<li>
							<a asp-controller="OurService" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Our Service</span>
							</a>
						</li>
						<li>
							<a asp-controller="partner" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Partner</span>
							</a>
						</li>
						<li>
							<a asp-controller="partnerCounr" asp-action="index">
								<i class="bi bi-database"></i>
								<span class="menu-text">Partner Count</span>
							</a>
						</li>
					</ul>
				</div>
			</nav>
			<div class="app-container">

				<div class="app-header d-flex align-items-center">
					<div class="d-flex">
						<button class="btn btn-outline-primary me-2 toggle-sidebar" id="toggle-sidebar">
							<i class="bi bi-text-indent-left fs-5"></i>
						</button>
						<button class="btn btn-outline-primary me-2 pin-sidebar" id="pin-sidebar">
							<i class="bi bi-text-indent-left fs-5"></i>
						</button>
					</div>
					<div class="app-brand-sm d-md-none d-sm-block">
						<a href="index.html">
							<img src="~/admin/assets/images/logo-sm.svg" class="logo" alt="Bootstrap Gallery">
						</a>
					</div>
					<div class="header-actions">
						<div class="search-container d-lg-block d-none me-2">
							<input type="text" class="form-control" placeholder="Search" />
							<i class="bi bi-search"></i>
						</div>
						<div class="dropdown ms-2">
							<a class="dropdown-toggle d-flex p-2 border rounded-2" href="#!" role="button" data-bs-toggle="dropdown"
							   aria-expanded="false">
								<i class="bi bi-grid fs-4 lh-1"></i>
							</a>
							<div class="dropdown-menu dropdown-menu-end shadow">
								<div class="d-flex gap-2 m-2">
									<a href="javascript:void(0)" class="g-col-4 p-2 border rounded-2">
										<img src="~/admin/assets/images/brand-behance.svg" class="img-2x" alt="Admin Themes" />
									</a>
									<a href="javascript:void(0)" class="g-col-4 p-2 border rounded-2">
										<img src="~/admin/assets/images/brand-gatsby.svg" class="img-2x" alt="Admin Themes" />
									</a>
									<a href="javascript:void(0)" class="g-col-4 p-2 border rounded-2">
										<img src="~/admin/assets/images/brand-google.svg" class="img-2x" alt="Admin Themes" />
									</a>
									<a href="javascript:void(0)" class="g-col-4 p-2 border rounded-2">
										<img src="~/admin/assets/images/brand-bitcoin.svg" class="img-2x" alt="Admin Themes" />
									</a>
									<a href="javascript:void(0)" class="g-col-4 p-2 border rounded-2">
										<img src="~/admin/assets/images/brand-dribbble.svg" class="img-2x" alt="Admin Themes" />
									</a>
								</div>
							</div>
						</div>
						<div class="dropdown ms-2">
							<a class="dropdown-toggle d-flex p-2 border rounded-2" href="#!" role="button" data-bs-toggle="dropdown"
							   aria-expanded="false">
								<i class="bi bi-bell fs-4 lh-1"></i>
							</a>
							<div class="dropdown-menu dropdown-menu-end shadow">
								<div class="dropdown-item">
									<div class="d-flex py-2 border-bottom">
										<img src="~/admin/assets/images/user.png" class="img-4x me-3 rounded-3" alt="Admin Theme" />
										<div class="m-0">
											<h6 class="mb-1">Sophie Michiels</h6>
											<p class="mb-2">Membership has been ended.</p>
											<p class="small m-0 text-secondary">Today, 07:30pm</p>
										</div>
									</div>
								</div>
								<div class="dropdown-item">
									<div class="d-flex py-2 border-bottom">
										<img src="~/admin/assets/images/user2.png" class="img-4x me-3 rounded-3" alt="Admin Theme" />
										<div class="m-0">
											<h6 class="mb-1">Sophie Michiels</h6>
											<p class="mb-2">Congratulate, James for new job.</p>
											<p class="small m-0 text-secondary">Today, 08:00pm</p>
										</div>
									</div>
								</div>
								<div class="dropdown-item">
									<div class="d-flex py-2">
										<img src="~/admin/assets/images/user1.png" class="img-4x me-3 rounded-3" alt="Admin Theme" />
										<div class="m-0">
											<h6 class="mb-1">Sophie Michiels</h6>
											<p class="mb-2">Lewis added new schedule release.</p>
											<p class="small m-0 text-secondary">Today, 09:30pm</p>
										</div>
									</div>
								</div>
								<div class="d-grid m-3">
									<a href="javascript:void(0)" class="btn btn-primary">View all</a>
								</div>
							</div>
						</div>
						<div class="dropdown ms-2">
							<a href="settings.html" data-bs-toggle="tooltip" data-bs-placement="bottom"
							   data-bs-custom-class="custom-tooltip-info" data-bs-title="Settings"
							   class="d-flex p-2 border rounded-2">
								<i class="bi bi-gear fs-4 lh-1"></i>
							</a>
						</div>
						<div class="dropdown ms-3">
							<a id="userSettings" class="dropdown-toggle d-flex py-2 align-items-center ps-3 border-start" href="#!"
							   role="button" data-bs-toggle="dropdown" aria-expanded="false">
								<span class="d-none d-md-block me-2">Michelle Max</span>
								<img src="~/admin/assets/images/user.png" class="rounded-circle img-3x" alt="Bootstrap Gallery" />
							</a>
							<div class="dropdown-menu dropdown-menu-end shadow">
								<a class="dropdown-item d-flex align-items-center" href="profile.html">
									<i class="bi bi-person fs-4 me-2"></i>Profile
								</a>
								<a class="dropdown-item d-flex align-items-center" href="settings.html">
									<i class="bi bi-gear fs-4 me-2"></i>Account Settings
								</a>
								<a class="dropdown-item d-flex align-items-center" href="login.html">
									<i class="bi bi-escape fs-4 me-2"></i>Logout
								</a>
							</div>
						</div>
					</div>
				</div>
				<div class="app-hero-header d-flex align-items-start">

					<ol class="breadcrumb d-none d-lg-flex align-items-center">
						<li class="breadcrumb-item">
							<i class="bi bi-house"></i>
							<a href="index.html">Home</a>
						</li>
						<li class="breadcrumb-item" aria-current="page">Dashboard</li>
					</ol>
@* 					<div class="ms-auto d-flex flex-row">
						<div class="d-flex flex-column me-5 text-end">
							<p class="m-0 text-secondary">Sales</p>
							<h3 class="m-0">5,58,560</h3>
						</div>
						<div class="d-flex flex-column text-end">
							<p class="m-0 text-secondary">Income</p>
							<h3 class="m-0">$65,80,990</h3>
						</div>
					</div> *@
				</div>
				<div class="app-body">
				@RenderBody()
				</div>
				<div class="app-footer">
					<span>© Bootstrap Gallery 2023</span>
				</div>
			</div>
		</div>
	</div>

	<script src="~/admin/assets/js/jquery.min.js"></script>
	<script src="~/admin/assets/js/bootstrap.bundle.min.js"></script>
	<script src="~/admin/assets/vendor/overlay-scroll/jquery.overlayScrollbars.min.js"></script>
	<script src="~/admin/assets/vendor/overlay-scroll/custom-scrollbar.js"></script>
	<script src="~/admin/assets/vendor/apex/apexcharts.min.js"></script>
	<script src="~/admin/assets/vendor/apex/custom/dash1/activity-report.js"></script>
	<script src="~/admin/assets/vendor/apex/custom/dash1/deals.js"></script>
	<script src="~/admin/assets/vendor/apex/custom/dash1/sparkline.js"></script>
	<script src="~/admin/assets/vendor/apex/custom/dash1/sparkline2.js"></script>
	<script src="~/admin/assets/vendor/rating/raty.js"></script>
	<script src="~/admin/assets/vendor/rating/raty-custom.js"></script>
	<script src="~/admin/assets/js/custom.js"></script>
	@RenderSection("Scripts", required: false)
</body>

</html>